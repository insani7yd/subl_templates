vector<int> primes;

void sieve(int n){
    bool a[n+1]={0};
	for(int i=2;i<N+1;i++){
		if(!a[i]) {
			primes.pb(i);
			for(int j=i+i;j<N+1;j+=i){
				a[j]=1;
			}
		}
	}
}
